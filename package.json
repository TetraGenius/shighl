{
  "name": "shighl",
  "version": "1.0.8",
  "description": "S-olid HIGH L-evel",
  "author": "Spoggy",
  "license": "MIT",
  "repository": "https://github.com/scenaristeur/shighl",
  "keywords": [
    "Solid",
    "Linked Data"
  ],
  "scripts": {
    "test": "mocha --require @babel/register ----timeout 60000",
    "prepublishOnly": "npm run build && npm run test:ls && npm run test:file",
    "build": "webpack",
    "build:dev": "webpack --mode development --watch",
    "build:docs": "jsdoc2md src/*.js > docs/JSdoc/api.md",
    "lint": "standard \"src/**/*.js\" \"test/**/*.js\"",
    "lint:fix": "npm run lint -- --fix",
    "start": "webpack-dev-server -d --hot --config webpack.config.js --watch"
  },
  "main": "dist/node/shighl.bundle.js",
  "browser": "dist/window/shighl.bundle.js",
  "module": "src/index.js",
  "browserslist": [
    "> 0.25%",
    "not dead"
  ],
  "standard": {
    "globals": [
      "jest",
      "test",
      "describe",
      "beforeAll",
      "beforeEach",
      "afterAll",
      "afterEach",
      "expect"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.4",
    "debug": "^4.1.1",
    "jest": "^24.9.0",
    "jsdoc-to-markdown": "^5.0.3",
    "regenerator-runtime": "^0.13.5",
    "solid-auth-cli": "^1.0.12",
    "solid-auth-client": "^2.4.1",
    "solid-namespace": "^0.2.0",
    "solid-rest": "^1.1.3",
    "standard": "^14.3.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@ldflex/rdflib": "^1.0.0",
    "@rdfjs/data-model": "^1.1.2",
    "@solid/query-ldflex": "^2.10.0",
    "chai": "^4.2.0",
    "fs": "0.0.1-security",
    "mocha": "^7.1.1",
    "n3": "^1.3.5",
    "nyc": "^15.0.0",
    "rdf-namespaces": "^1.8.0",
    "tripledoc": "^4.3.0"
  }
}
